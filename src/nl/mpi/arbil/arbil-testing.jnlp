    <resources>
        <j2se version="1.5+"/>
        <jar href="arbil-1-3-17707.jar" main="true" download="eager"/>
        <jar href="lib/corpusstructure-1.6.1.jar" download="eager"/>
        <jar href="lib/imdiapi-1.0.6.jar" download="eager"/>
        <jar href="lib/log4j-1.2.14.jar" download="eager"/>
        <jar href="lib/saxon8.jar" download="eager"/>
        <jar href="lib/saxon8-dom.jar" download="eager"/>
        <jar href="lib/typecheck-1.5.16185.jar" download="eager"/>
        <jar href="lib/xalan-2.6.0.jar" download="eager"/>
        <jar href="lib/xercesImpl-2.9.0.jar" download="eager"/>
        <jar href="lib/bcprov-jdk15-143.jar" download="eager"/>
        <jar href="lib/slcshttps_jdk15_v0.2.jar" download="eager"/>
    </resources>
    <resources os="windows">
        <nativelib href="lib/ImageMagick-6.5.9-Win32.jar"/>
