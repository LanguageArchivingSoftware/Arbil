<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://www.mpi.nl/tg/j2se/jnlp/arbil/" href="arbil-testing.jnlp">
    <information>
        <title>Arbil</title>
        <icon href="http://www.mpi.nl/tg/j2se/jnlp/arbil/arbil-testing128x128.png" />
        <offline-allowed/>
        <shortcut online="false">
            <desktop />
            <!--<menu sub-menu="Language Archiving Tools" />-->
            <!-- <menu sub-menu="" /> -->
            <!-- <menu /> -->
        </shortcut>
        <vendor>Max Planck Institute for Psycholinguistics</vendor>
        <homepage href="http://www.lat-mpi.eu/tools/imdi/Arbil/"/>
